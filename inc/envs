# Environment variables common to zsh and bash
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# PATH
PATH=$HOME/bin:$HOME/dotfiles/bin:$HOME/usr/miniconda2/bin:/usr/local/sbin:/usr/local/bin:$PATH


# brew golang
PATH=$PATH:/usr/local/opt/go/bin:/usr/local/opt/go/libexec/bin

export PATH

#Golang
# brew
export GOPATH=$HOME/code/golang/go

#Java stuff
export JAVA_HOME=$(/usr/libexec/java_home -v 1.8)

#Editor
export EDITOR=/usr/local/bin/vim
